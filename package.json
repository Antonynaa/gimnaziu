{
    "private": true,
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "copy-google-fonts": "copyfiles -u 1 node_modules/get-google-fonts/dist/**/* public/lib/get-google-fonts",
        "copy-animate-css": "copyfiles -u 2 node_modules/animate.css/animate.min.css public/lib/animate.css",
        "copy-bootstrap-icons": "copyfiles -u 2 node_modules/bootstrap-icons/font/**/* public/lib/bootstrap-icons", 
        "copy-owl-carousel": "copyfiles -u 2 node_modules/owl.carousel/dist/**/* public/lib/owl.carousel",
        "copy-waypoints": "copyfiles -u 2 node_modules/waypoints/lib/**/* public/lib/waypoints",
        "copy-wowjs": "copyfiles -u 2 node_modules/wowjs/dist/**/* public/lib/wowjs",
        "copy-easing": "copyfiles -u 2 node_modules/jquery.easing/*.js public/lib/easing",
        "copy-jquery": "copyfiles -u 2 node_modules/jquery/dist/jquery.min.js public/js/jquery.min.js",
        "copy-bootstrap-css": "copyfiles -u 2 node_modules/bootstrap/dist/css/bootstrap.min.css public/css",
        "copy-style-css": "copyfiles -u 1 src/css/style.css public/css",
        "copy-all-css": "npm run copy-bootstrap-css && npm run copy-style-css"
    },
    "devDependencies": {
        "@popperjs/core": "^2.11.6",
        "axios": "^1.1.2",
        "bootstrap": "^5.3.3",
        "copyfiles": "^2.4.1",
        "laravel-vite-plugin": "^0.7.8",
        "sass": "^1.56.1",
        "vite": "^4.0.0"
    },
    "dependencies": {
        "animate.css": "^4.1.1",
        "bootstrap-icons": "^1.11.3",
        "get-google-fonts": "^1.2.2",
        "jquery": "^3.7.1",
        "jquery.easing": "^1.4.1",
        "owl.carousel": "^2.3.4",
        "waypoints": "^4.0.1",
        "wowjs": "^1.1.3"
    }
}
